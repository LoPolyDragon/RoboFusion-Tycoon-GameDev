{
  "$schema": "https://rojo.space/schemas/project-0.6.0.json",
  "name": "robofusion-mine",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "ClientUtils": { "$path": "Shared/ReplicatedStorage/ClientUtils" },

      "SharedModules": {
        "$className": "Folder",

        "GameConstants": {
          "$path": "Shared/ReplicatedStorage/SharedModules/GameConstants"
        }
      }
    },

    "ServerStorage": {
      "RobotTemplates": { "$path": "Shared/ServerStorage/RobotTemplates" }
    },

    "ServerScriptService": {
      "ServerModules": { "$path": "Shared/ServerScriptService/ServerModules" },
      "$path": "Mine Game/src/ServerScriptService"
    },

    "StarterPlayer": {
      "StarterPlayerScripts": { "$path": "Mine Game/src/StarterPlayerScripts" }
    }
  }
}